{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "description": "Delivery channel",
  "additionalProperties": false,
  "properties": {
    "type": {
      "type": "string",
      "description": "Type of channel"
    },
    "from": {
      "type": "string",
      "description": "Message sender"
    },
    "to": {
      "type": "string",
      "description": "Message receiver"
    }
  },
  "required": [
    "type",
    "from",
    "to"
  ]
}
